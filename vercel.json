{
  "version": 2,
  "builds": [
    {
      "src": "api/update-data.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/update-data",
      "dest": "/api/update-data.js",
      "methods": ["POST", "OPTIONS"]
    },
    {
      "src": "/admin",
      "dest": "/admin.html"
    },
    {
      "src": "/table",
      "dest": "/table.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "api/update-data.js": {
      "maxDuration": 30,
      "memory": 1024,
      "runtime": "nodejs18.x"
    }
  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
